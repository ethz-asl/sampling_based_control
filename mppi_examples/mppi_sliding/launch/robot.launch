<?xml version="1.0" ?>
<launch>
    <arg name="robot_ip" default="$(env FRANKA_IP)"/>
    <arg name="object" default="shelf"/>
    <arg name="experiment_name" default="panda_manipulation"/>

    <include file="$(find mppi_sliding)/launch/load.launch">
        <arg name="debug" value="false"/>
        <arg name="object" value="$(arg object)"/>
        <arg name="description_file" value="$(find mppi_sliding)/data/panda.urdf.xacro"/>
    </include>

    <include file="$(find franka_gripper)/launch/franka_gripper.launch">
        <arg name="robot_ip" value="$(arg robot_ip)" />
    </include>

    <!-- controller manager-->
    <param name="/config_file" value="$$(find mppi_sliding)/config/experiments/controllers_mug.yaml"/>
    <node name="controller_spawner" pkg="controller_manager" type="spawner" output="screen" args="mppi_sliding"> 
        <!--rosparam command="load" file="$(find mppi_sliding)/params/params.yaml"/-->
        <!--param name="activation_file" value="/home/boyang/.raisim/activation.raisim"/-->
        <rosparam command="load" file="$(find mppi_sliding)/config/experiments/controllers_mug.yaml" />
        <param name="experiment_name" value="$(arg experiment_name)"/>
    </node>

</launch>